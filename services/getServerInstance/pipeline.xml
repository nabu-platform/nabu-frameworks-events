<structure name="pipeline">
    <structure name="input">
        <field name="serverName" type="java.lang.String"/>
        <field name="serverGroup" type="java.lang.String"/>
    </structure>
    <structure name="output">
        <field name="serverInstanceId" pattern="[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}|[0-9a-fA-F]{32}" type="java.util.UUID"/>
    </structure>
    <structure collectionName="serverGroups" definition="nabu.frameworks.events.database.serverGroup" minOccurs="0" name="existingGroup"/>
    <structure collectionName="serviceInstances" definition="nabu.frameworks.events.database.serverInstance" minOccurs="0" name="existingServer"/>
</structure>
